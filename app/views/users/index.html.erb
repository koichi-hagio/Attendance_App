<% provide(:title, "All Users") %>
  <h1>ユーザー一覧</h1>
  


<div class="col-lg-10">
  <div class="pull-left">
    <%=  form_with model: @user do |f| %>
      <%= f.file_field :upload_file %> <!-- csvのみインポート可にする -->
      <%= f.submit "CSVをインポート", class: "btn btn-primary btn-block btn-basic-info" %>
    <% end %>
  </div>
</div>

